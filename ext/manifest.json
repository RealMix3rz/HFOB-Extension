{
  "name": "HF Orange Background",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Extension that changes HackForum's background orange.",
  "homepage_url": "https://github.com/RealMix3rz",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://hackforums.net/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://hackforums.net/*"
      ],
      "css": [
        "css/inject.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "img/orange.png"
  ]
}